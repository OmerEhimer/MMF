<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

    <!-- Sequences for form -->
       <record id="seq_convert_form" model="ir.sequence">
            <field name="name">Convert Form</field>
            <field name="code">mmf.form.sequence</field>
            <field name="prefix">CN</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        
    <!-- Sequences for Invoices -->
        <record id="seq_invoicet_form" model="ir.sequence">
                <field name="name">Invoice Sequence</field>
                <field name="code">mmf.invoice.sequence</field>
                <field name="prefix">INV</field>
                <field name="padding">8</field>
                <field name="company_id" eval="False"/>
            </record>
    </data>
</odoo>
